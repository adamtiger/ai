        <Label x:Name="label1" Content="Columns" HorizontalAlignment="Left" Height="24" Margin="5,41,0,0" VerticalAlignment="Top" Width="66" RenderTransformOrigin="0.5,0.5"/>
        <TextBox x:Name="columns" HorizontalAlignment="Left" Height="31" Margin="63,41,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="67"/>
        <Label x:Name="label2" Content="Start" HorizontalAlignment="Left" Height="27" Margin="10,72,0,0" VerticalAlignment="Top" Width="38"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="27" Margin="63,77,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="18" RenderTransformOrigin="2.212,0.663"/>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Height="27" Margin="108,77,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="22"/>
        <Label x:Name="label3" Content="Target" HorizontalAlignment="Left" Height="28" Margin="5,99,0,0" VerticalAlignment="Top" Width="43"/>
        <TextBox x:Name="textBox2" HorizontalAlignment="Left" Height="23" Margin="63,104,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="18"/>
        <TextBox x:Name="textBox3" HorizontalAlignment="Left" Height="23" Margin="108,104,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="22"/>
        <Label x:Name="label4" Content="Alpha" HorizontalAlignment="Left" Height="25" Margin="10,142,0,0" VerticalAlignment="Top" Width="46"/>
        <TextBox x:Name="textBox4" HorizontalAlignment="Left" Height="25" Margin="86,142,0,0" TextWrapping="Wrap" Text="0.02" VerticalAlignment="Top" Width="44"/>
        <Label x:Name="label5" Content="#Episodes" HorizontalAlignment="Left" Height="27" Margin="10,172,0,0" VerticalAlignment="Top" Width="71"/>
        <TextBox x:Name="textBox5" HorizontalAlignment="Left" Height="27" Margin="86,172,0,0" TextWrapping="Wrap" Text="100" VerticalAlignment="Top" Width="44"/>
        <ProgressBar HorizontalAlignment="Center" Height="13" Margin="48,255,30,195" Width="112" ValueChanged="ProgressBar_ValueChanged"/>

    </Grid>
</Window>
